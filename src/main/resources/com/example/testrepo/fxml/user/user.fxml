<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" stylesheets="@../../css/user.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.testrepo.controllers.user.userController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <HBox id="menu-bar" alignment="CENTER_LEFT" prefHeight="48.0" prefWidth="800.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="674.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Button mnemonicParsing="false" prefHeight="27.0" prefWidth="33.0" styleClass="menu-icons" text="Dashboard">
                           <graphic>
                              <ImageView fitHeight="12.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true" />
                           </graphic>
                        </Button>
                        <Button mnemonicParsing="false" prefHeight="23.0" prefWidth="32.0" styleClass="menu-icons" text="Movies">
                           <graphic>
                              <ImageView fitHeight="13.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true" />
                           </graphic>
                        </Button>
                     </children>
                  </HBox>
                  <HBox prefHeight="49.0" prefWidth="39.0">
                     <children>
                        <Button alignment="CENTER" mnemonicParsing="false" prefHeight="50.0" prefWidth="48.0" styleClass="menu-icons">
                           <graphic>
                              <ImageView fitHeight="41.0" fitWidth="37.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../images/logout-icon.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </HBox>
         </top>
         <center>
            <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <fx:include fx:id="browseMoviesUI" source="user-browse-movies.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0" visible="false">
                     <children>
                        <fx:include fx:id="detailedMovieUI" source="user-detailed-movie.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
                  <AnchorPane prefHeight="437.0" prefWidth="800.0" visible="false">
                     <children>
                        <fx:include fx:id="allBookingsUI" source="user-bookings.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
               </children></StackPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
